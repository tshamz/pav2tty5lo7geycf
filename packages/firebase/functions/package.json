{
  "private": true,
  "name": "functions",
  "version": "0.0.1",
  "engines": {
    "node": "12"
  },
  "scripts": {
    "deploy": "firebase deploy --only functions",
    "dev": "firebase emulators:start --export-on-exit ./.data --import ./.data",
    "triggers:disable": "curl -X PUT localhost:4400/functions/disableBackgroundTriggers",
    "triggers:enable": "curl -X PUT localhost:4400/functions/enableBackgroundTriggers",
    "serve": "firebase emulators:start --only functions",
    "shell": "firebase functions:shell",
    "start": "npm run shell",
    "logs": "firebase functions:log"
  },
  "dependencies": {
    "@services/firebase": "workspace:^0.0.1",
    "@services/logger": "workspace:^0.0.1",
    "@services/twilio": "workspace:^0.0.1",
    "@services/utils": "workspace:^0.0.1",
    "deep-equal": "^2.0.5",
    "firebase": "^8.2.3",
    "firebase-admin": "^9.4.2",
    "firebase-functions": "^3.13.1",
    "lodash": "^4.17.20",
    "node-fetch": "^2.6.1",
    "puppeteer": "^5.5.0",
    "puppeteer-extra": "^3.1.16",
    "puppeteer-extra-plugin-stealth": "^2.6.5",
    "tinyurl": "^1.1.7",
    "wait": "^0.4.2"
  }
}
