{
  "private": true,
  "name": "status",
  "version": "0.0.1",
  "main": "index.js",
  "engines": {
    "node": "12",
    "yarn": "2.4.0"
  },
  "scripts": {
    "dev": "yarn g:dev",
    "watch": "yarn nodemon $INIT_CWD --watch $INIT_CWD --watch ../../",
    "build": "yarn bundle -a .build.zip",
    "deploy": "yarn build && gcloud beta app deploy status.yaml --quiet --stop-previous-version && yarn postdeploy",
    "postdeploy": "rm -rf .build.zip",
    "start": "yarn node bundle/entrypoint.js"
  },
  "dependencies": {
    "@services/debug": "workspace:packages/@services/debug",
    "@services/env": "workspace:packages/@services/env",
    "@services/express": "workspace:packages/@services/express",
    "@services/logger": "workspace:packages/@services/logger"
  },
  "devDependencies": {
    "nodemon": "^2.0.7"
  }
}
