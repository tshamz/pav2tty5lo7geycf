    "deploy": "firebase deploy --only functions"
  },
  "main": "index.js",
  "dependencies": {
    "services": "0.0.1",
    "deep-equal": "^2.0.5",
    "lodash": "^4.17.20",
    "puppeteer": "^5.5.0",
    "puppeteer-extra": "^3.1.15",
    "puppeteer-extra-plugin-stealth": "^2.6.5",
    "tinyurl": "^1.1.7"
  }
}
