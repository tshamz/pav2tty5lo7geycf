{
  "private": true,
  "name": "functions",
  "version": "0.0.1",
  "workspaces": [
    "services"
  ],
  "engines": {
    "node": "12"
  },
  "scripts": {
    "links:create": "./build/create-links.sh firebase/functions",
    "links:remove": "./build/remove-links.sh firebase/functions",
