{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es6",
    "module": "esnext",
    "moduleResolution": "node",
    "paths": {
      "*": ["*"]
    }
  },
  // prettier-ignore
  "include": [
    "./",
    "servers/**/*",
    "firebase/**/*",
  ],
  // prettier-ignore
  "exclude": [
    ".*",
    "node_modules",
    "**/node_modules",
  ]
}
